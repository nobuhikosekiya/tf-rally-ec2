aws_region  = "ap-northeast-1"
aws_profile = "default"
prefix      = "rally-test"

# EC2 Configuration
ec2_instance_type   = "m5.large"
ec2_ami_id          = "ami-0599b6e53ca798bb2"
ssh_public_key_path = "~/.ssh/id_rsa.pub"
ebs_volume_size     = 100

# Elasticsearch Configuration
elasticsearch_url = "https://your-elasticsearch-endpoint:9200"
elastic_api_key   = "your-elastic-api-key"

# Tags
default_tags = {
  division   = "platform"
  org        = "engineering"
  keep-until = "2026-01-01"
  team       = "observability"
  project    = "elastic-rally"
}